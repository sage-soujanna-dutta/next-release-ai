# Required Environment Variables
JIRA_EMAIL=your-email@company.com
JIRA_DOMAIN=your-domain.atlassian.net
JIRA_TOKEN=your_jira_api_token_here
JIRA_BOARD_ID=your_board_id
JIRA_SPRINT_NUMBER=SCNT-2025-XX
 
# GitHub Configuration
GH_REPOSITORY=your-org/your-repo
GH_TOKEN=ghp_your_github_token_here
 
# Confluence Configuration
CONFLUENCE_USERNAME=your-confluence-email@company.com
CONFLUENCE_PAT=your_confluence_personal_access_token_here
CONFLUENCE_SPACE=your_confluence_space_key
JIRA_CONFLUENCE_DOMAIN=your-domain.atlassian.net
CONFLUENCE_PARENT_PAGE_ID=your_parent_page_id
 
# Optional Configuration
JIRA_FETCH_COMMITS_DATE=2024-01-01T00:00:00Z
OUTPUT_DIR=./output
TEAMS_WEBHOOK_URL=https://your-tenant.webhook.office.com/webhookb2/your-webhook-url-here

# Default values (optional)
DEFAULT_SPRINT_NUMBER=SCNT-2025-XX

#Azure Configuration
AZURE_ORG_URL=https://dev.azure.com/YourOrganization
AZURE_PROJECT=YourProject
AZURE_PAT=your_azure_devops_personal_access_token_here
QA_ENV_NAME=QA
PIPELINE_NAMES=YourPipeline1,YourPipeline2,YourPipeline3
RELEASE_BRANCH_PATTERNS=Release-sprint-2025-XX,release-sprint-2025-XX
